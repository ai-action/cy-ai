{
  "name": "cy-ai",
  "version": "1.0.0-alpha",
  "description": "Cypress AI command",
  "author": "Mark <mark@remarkablemark.org> (https://remarkablemark.org)",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf cjs coverage docs",
    "docs": "typedoc",
    "docs:watch": "npm run docs -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:package": "publint",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm run test:ci && npm run test:esm && npm run clean && npm run build",
    "test": "jest --watch",
    "test:ci": "CI=true jest --ci --colors --coverage",
    "test:esm": "npm run build && node --test __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-action/cy-ai.git"
  },
  "bugs": {
    "url": "https://github.com/ai-action/cy-ai/issues"
  },
  "keywords": [
    "cypress",
    "ai",
    "command",
    "test"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.26",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "eslint": "9.27.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "16.1.0",
    "prettier": "3.5.3",
    "publint": "0.3.12",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typedoc": "0.28.5",
    "typescript": "5.8.3"
  },
  "files": [
    "cjs/",
    "src/"
  ],
  "license": "MIT"
}
