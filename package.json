{
  "name": "cy-ai",
  "version": "0.6.3",
  "description": "Cypress AI command",
  "author": "Mark <mark@remarkablemark.org> (https://remarkablemark.org)",
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf cypress/{downloads,screenshots,videos} docs lib",
    "cypress:open": "cypress open",
    "docs": "typedoc",
    "docs:watch": "npm run docs -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:package": "publint",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm run clean && npm run build",
    "test": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-action/cy-ai.git"
  },
  "bugs": {
    "url": "https://github.com/ai-action/cy-ai/issues"
  },
  "keywords": [
    "cy-ai",
    "cypress",
    "ai",
    "llm",
    "command",
    "test",
    "ui",
    "automation"
  ],
  "dependencies": {
    "@langchain/core": "^0.3",
    "@langchain/ollama": "^1.0",
    "dompurify": "3"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/compat": "2.0.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@types/node": "24.10.1",
    "cypress": "15.7.0",
    "eslint": "9.39.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.5.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "publint": "0.3.15",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0"
  },
  "peerDependencies": {
    "cypress": "*"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "license": "MIT"
}
